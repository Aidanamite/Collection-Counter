{
  "name": "Collection Counter",
  "author": "Aidanamite",
  "description": "Counts the items collected from the ocean",
  "version": "1.0.1",
  "license": "GNU AGPLv3",
  "icon": "icon.png",
  "banner": "banner.jpg",
